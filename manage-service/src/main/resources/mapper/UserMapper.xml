<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.fw.manage.dao.UserMapper">

    <insert id="insert" parameterType="com.fw.manage.entity.User" useGeneratedKeys="true" keyProperty="id">
        insert into test (name, type) values (#{name}, #{type})
    </insert>

    <select id="selectById" parameterType="java.lang.Long" resultType="com.fw.manage.entity.User">
        select *
        from test
        where id = #{id}
    </select>
</mapper>
