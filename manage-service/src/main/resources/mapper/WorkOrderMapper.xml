<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.fw.manage.dao.WorkOrderMapperDao">

    <select id="selectByPage" resultType="com.fw.manage.entity.WorkOrder">
        select * from work_order where is_delete = 0 order by id desc limit #{start},#{pageSize}
    </select>
</mapper>
